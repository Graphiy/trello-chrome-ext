{
    "name": "Trello Links Catcher",
    "description": "Catch Trello links and open in \"Trello External Window\" application.",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "management",
        "https://trello.com/*"
    ],
    "browser_action": {
        "default_title": "Trello Links Catcher",
        "default_icon": "trello-mark-blue-16.png"
    },
    "manifest_version": 2
}
